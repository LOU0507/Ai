package javaBasic.ch02.sec10;

public class DataType2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        // 논리형 boolean 불린형
		boolean b1 = true; //대문자도 안되고 다른 글자도 안된다
		boolean b2 = false;
		
		boolean b3 = 10 < 8;
		System.out.println(b3);
		
		boolean b4 = 100 == 100;
		System.out.println( b4 );
	}

}
