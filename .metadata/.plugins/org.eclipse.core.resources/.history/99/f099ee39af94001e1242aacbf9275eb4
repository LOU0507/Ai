package javaBasic2.ch05.sec03;

public class 확인문제5장 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
   int[][] array = {
		   {95,86},
		   {83,92,96},
		   {78,83,93,87,88}
   };
   
   System.out.println(array.length); // array의 길이를 물어보는거라  3개
   System.out.println(array[2].length); //array[2]의 길이를 구해라니깐 783
   
   
   
   //7번
   int [] array7 = {1,5,3,8,2};
   int max = 0;
   for ( int i=0 ; i<array7.length ; i++) {
	  if (max < array7[i])
	   max = array7[i]; 
   }
   System.out.println(max);
   
   int min = 1000000;
   for ( int i=0 ; i<array7.length ; i++) {
	  if (min > array7[i])
	   min = array7[i]; 
   }
   System.out.println(min);
   
   
   //8번
  int[][] array8 = {
		  {95,86},
		  {83,92,96},
		  {78,83,93,87,88}
  };
  int sum = 0;
  for (int i=0 ; i<array8.length ; i++) {
	  for (int j=0 ; j < array8[i] ; j++)
	sum = sum + array[i][j] ;
	  
  }
   System.out.println(sum);
   
   
   
   
   
   
	}

}

 