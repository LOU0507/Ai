package javaBasic.ch01.sec15;

public class DataTypeEx {

	public static void main(String[] args) {
   // TODO Auto-generated method stub
   // 자료형 기본형 (정수형 byte int long, 실수형 - float double, 논리형 - boolean, 문자형 - char)
		
   // 정수처리
  byte b1 = 127; //-128~127
  System.out.println ( b1 );
  
  short s1 = 32767; //-32768~32767
  long lol = 9000000000000000000l;
  
  byte b2=10;
  byte b3=20;
  
  int b23 = b2+b3; //에러의 이유는 더하기는 int로 처리하기 때문 / byte(1)형 int(4)형 byte(1)형 = int형임
	}

}
