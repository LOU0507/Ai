package ch02.sec09;

public class 논리연산자 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    boolean b1 = true;
    boolean b2 = false;
   
    System.out.println(b1 && b2); // && = and
    System.out.println(b1 || b2); // || = or
    System.out.println(b1^b2); // 참 거짓이면 참
    System.out.println(!b1); //!= 반대
    
    
	}

}
