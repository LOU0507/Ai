package javaBasic3.ch06.sec10;

public class Car {

	public Car() {
		super();
	}
	
	String company = "현대자동차";
	String model = "그랜저";
	int speed = 300;
	boolean start = true;
	String color = "black";
	int maxSpeed = 350;
	
	
	
	public Car(String company, String model, int speed, boolean start, String color, int maxSpeed) {
		super();
		this.company = company;
		this.model = model;
		this.speed = speed;
		this.start = start;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}
	
//	Tire tire = new Tire();

	
	
	
	
	
}

